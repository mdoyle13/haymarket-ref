<% @events.each do |event| %>
	<% unless event.id == @featured_event.id %>
   <div class="accordion">
		<h3><a href="#"><%= event.name %></a>
			<span class="ui-event-info"><%= event.date.strftime("%B %d, %Y") %><br /><%= event.venue %></span>
		</h3>
		<div>
			<%= event.description %>
			<%= event.location %>
			
			<p><%= link_to "For Tickets Click Here", event.purchase_link unless event.purchase_link.empty? %></p>
		</div>
   </div>
	<% end %>
 <% end %>