<ul class="slides">
<% @slides.each do |slide| %>
	<% if slide.is_video? %>
		<li>
			<iframe class="vimeo-player" id="player_<%= slide.id %>" src="<%= slide.video_embed %>?api=1&player_id=player_<%= slide.id %>" width="540" height="303" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
			<p class="flex-caption">Test</p>	
			</li>
	<% else %>
		<li><%= image_fu slide.photo %></li>
	<% end %>
<% end %>	
</ul>